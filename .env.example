# PostgreSQL Configuration
POSTGRES_PASSWORD=your_postgres_password_here

# Linkwarden Configuration
NEXTAUTH_SECRET=your_nextauth_secret_here_generate_with_openssl_rand_base64_32
NEXTAUTH_URL=http://localhost:4000/api/v1/auth

# Data Directory (absolute path where data will be stored)
LINK_DIR=/path/to/your/linkwarden/data

# OpenAI Configuration (for AI tagging feature)
# Get your API key from: https://platform.openai.com/api-keys
OPENAI_API_KEY=your_openai_api_key_here
OPENAI_MODEL=gpt-3.5-turbo

# Optional: Only needed for OpenAI-compatible providers (not official OpenAI)
# CUSTOM_OPENAI_BASE_URL=https://your-custom-openai-endpoint.com